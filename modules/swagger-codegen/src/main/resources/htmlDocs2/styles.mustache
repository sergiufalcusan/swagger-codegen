/* ------------------------------------------------------------------------------------------
* Content
* ------------------------------------------------------------------------------------------ */

* {
font-family: ApercuPro-Regular, sans-serif;
}
body {
min-width: 980px;
background-color: #191E24;
color: #d3d3d3;
}

body, p, a, div, th, td {
font-family: ApercuPro-Regular, sans-serif;
font-weight: 400;
font-size: 16px;
text-shadow: none !important;
}

td.code {
font-size: 14px;
font-family: ApercuPro-Regular, sans-serif;
font-style: normal;
font-weight: 400;
}

#content {
padding-top: 16px;
z-Index: -1;
margin-left: 290px;
}

code {
padding: 2px 4px;
color: #d14;
white-space: nowrap;
background-color: #292b36;
border: 1px solid #292b36;
}

p {
color: #d3d3d3;
}

h1 {
font-family: ApercuPro-Bold, sans-serif;
font-weight: normal;
font-size: 37px;
line-height: 50px;
margin: 0 0 10px 0;
padding: 0;
}

h2 {
font-family: ApercuPro-Regular, sans-serif;
font-weight: normal;
font-size: 24px;
line-height: 40px;
margin: 0 0 20px 0;
padding: 0;
}

section {
border-top: 1px solid #555;
padding: 30px 0;
}

section h1 {
font-family: ApercuPro-Regular, sans-serif;
font-weight: 700;
font-size: 32px;
line-height: 40px;
padding-bottom: 14px;
margin: 0 0 20px 0;
padding: 0;
}

article {
padding: 14px 0 30px 0;
}

article h1 {
font-family: ApercuPro-Bold, sans-serif;
font-weight: 600;
font-size: 24px;
line-height: 26px;
}

article h2 {
font-family: ApercuPro-Regular, sans-serif;
font-weight: 600;
font-size: 18px;
line-height: 24px;
margin: 0 0 10px 0;
}

article h3 {
font-family: ApercuPro-Regular, sans-serif;
font-weight: 600;
font-size: 16px;
line-height: 18px;
margin: 0 0 10px 0;
}

article h4 {
font-family: ApercuPro-Regular, sans-serif;
font-weight: 600;
font-size: 14px;
line-height: 16px;
margin: 0 0 8px 0;
}

table {
border-collapse: collapse;
width: 100%;
margin: 0 0 20px 0;
}

th {
background-color: #292b36;
text-align: left;
font-family: ApercuPro-Regular, sans-serif;
font-weight: 700;
padding: 4px 8px;
border: #555 1px solid;
}

td {
vertical-align: top;
padding: 2px 8px;
border: #555 1px solid;
}

#generator .content {
color: #b0b0b0;
border-top: 1px solid #555;
padding: 10px 0;
}

.label-optional {
float: right;
}

.open-left {
right: 0;
left: auto;
}

/* ------------------------------------------------------------------------------------------
* apidoc - intro
* ------------------------------------------------------------------------------------------ */

#apidoc .apidoc {
border-top: 1px solid #555;
padding: 30px 0;
}

#apidoc h1 {
font-family: ApercuPro-Regular, sans-serif;
font-weight: 700;
font-size: 32px;
line-height: 40px;
padding-bottom: 14px;
margin: 0 0 20px 0;
padding: 0;
}

#apidoc h2 {
font-family: ApercuPro-Bold, sans-serif;
font-weight: 600;
font-size: 22px;
line-height: 26px;
padding-top: 14px;
}

/* ------------------------------------------------------------------------------------------
* pre / code
* ------------------------------------------------------------------------------------------ */
pre {
background-color: #292b36;
color: #d3d3d3;
padding: 10px;
border-radius: 6px;
position: relative;
margin: 10px 0 20px 0;
}

code.language-text {
word-wrap: break-word;
}

pre.language-json {
overflow: auto;
}

pre.language-html {
margin: 40px 0 20px 0;
}

pre.language-html:before {
content: attr(data-type);
position: absolute;
top: -30px;
left: 0;
font-family: ApercuPro-Regular, sans-serif;
font-weight: 600;
font-size: 15px;
display: inline-block;
padding: 2px 5px;
border-radius: 6px;
text-transform: uppercase;
background-color: #3387CC;
color: #d3d3d3;
}

pre.language-html[data-type="get"]:before {
background-color: #65B042;
}

pre.language-html[data-type="put"]:before {
background-color: #e5c500;
}

pre.language-html[data-type="post"]:before {
background-color: #4070ec;
}

pre.language-html[data-type="delete"]:before {
background-color: #ed0039;
}

pre.language-api .str {
color: #d3d3d3;
}

pre.language-api .pln,
pre.language-api .pun {
color: #65B042;
}

pre code {
display: block;
font-size: 14px;
font-family: ApercuPro-Regular, sans-serif;
font-style: normal;
font-weight: 400;
}

pre code.sample-request-response-json {
white-space: pre-wrap;
max-height: 500px;
overflow: auto;
}

/* ------------------------------------------------------------------------------------------
* Sidenav
* ------------------------------------------------------------------------------------------ */
.sidenav {
width: 270px;
margin: 0;
padding: 20px 0 20px 20px;
position: fixed;
top: 0;
left: 0;
bottom: 0;
overflow-x: hidden;
overflow-y: auto;
background-color: #242e33;
border-color: #34434b;
border-style: solid;
border-width: 1px;
z-index: 10;
}

*::-webkit-scrollbar {
width: 5px;
}
*::-webkit-scrollbar-track {
-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
background-color: #2b373d;
}
*::-webkit-scrollbar-thumb {
background-color: #3d4e58;
border-radius: 50vw;
}

.sidenav > li > a {
display: block;
width: 192px;
margin: 0;
padding: 2px 11px;
border: 0;
font-family: ApercuPro-Light, sans-serif;
font-size: 16px;
color: #d3d3d3;
line-height: 27px;
}

.sidenav > li.nav-header > a {
padding: 5px 10px;
width: 250px;
font-family: ApercuPro-Bold, sans-serif;
font-weight: 700;
font-size: 17px;
color: #0b87da;
line-height: 27px;
}

.sidenav > li.nav-header.active > a {
border-right: 3px solid rgb(11, 135, 218);
color: #d3d3d3;
}

.sidenav > .active > a {
position: relative;
z-index: 2;
}

.sidenav > li > a,
.sidenav > li > a:visited,
.sidenav > li > a:active,
.sidenav > li > a:focus,
.sidenav > li > a:hover {
    background-color: transparent;
    color: #d3d3d3;
}

.sidenav > li.has-modifications a {
border-right: #60d060 4px solid;
}

.sidenav > li.is-new a {
border-left: #e5e5e5 4px solid;
}


/* ------------------------------------------------------------------------------------------
* Tabs
* ------------------------------------------------------------------------------------------ */
ul.nav-tabs {
margin: 0;
}

/* ------------------------------------------------------------------------------------------
* Print
* ------------------------------------------------------------------------------------------ */

@media print {

#sidenav,
#version,
#versions,
section .version,
section .versions {
display: none;
}

#content {
margin-left: 0;
}

a {
text-decoration: none;
color: #d3d3d3;
}

a:after {
content: " [" attr(href) "] ";
}

p {
color: #000000
}

pre {
background-color: #d3d3d3;
color: #000000;
padding: 10px;
border: #d3d3d3 1px solid;
border-radius: 6px;
position: relative;
margin: 10px 0 20px 0;
}

} /* /@media print */


.doc-chapter
{
display:none;
background-color: #eee;
border-radius: 1px;
padding: 10px;
margin-bottom: 20px;
}

hr {
border-bottom: 1px solid #555;
border-top: none;
}

